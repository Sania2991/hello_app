<% provide(:title, 'Sign up') %>
<h1>Регистрация:</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
      <!-- Обрати внимани на "f: f" !!!!!! -->
      <%= render 'form', f: f, object: f.object %>
      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>